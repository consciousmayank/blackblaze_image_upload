{
	"name": "b2-image-tools",
	"version": "0.1.0",
	"description": "Backblaze B2 image upload and viewing components for Svelte/SvelteKit",
	"author": "Your Name",
	"license": "MIT",
	"keywords": [
		"backblaze",
		"b2",
		"svelte",
		"sveltekit",
		"image",
		"upload",
		"viewer"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/yourusername/b2-image-tools.git"
	},
	"exports": {
		"./components": "./src/lib/index.js",
		"./api": "./src/lib/api.js",
		"./styles": "./src/lib/styles.css"
	},
	"files": [
		"src/lib"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"package": "svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^1.20.0",
		"@sveltejs/package": "^2.2.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^4.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.5"
	},
	"dependencies": {
		"@types/node": "^22.14.1"
	},
	"type": "module",
	"engines": {
		"node": ">=16.0.0"
	}
}
